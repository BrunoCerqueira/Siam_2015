<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
     <application>
	  <resource-bundle>
		<base-name>resources.ApplicationMessages</base-name>
		<var>msg</var>
	   </resource-bundle>
     </application>
      <navigation-rule>
	<from-view-id>*</from-view-id>
	<navigation-case>
	
		<from-outcome>administrarPaciente</from-outcome>
		<to-view-id>pages/administrarPaciente.xhtml</to-view-id>
	</navigation-case>
	
	<navigation-case>
	
		<from-outcome>pesquisarPaciente</from-outcome>
		<to-view-id>pages/pesquisarPaciente.xhtml</to-view-id>
	</navigation-case>
	<navigation-case>
	
		<from-outcome>graficoPaciente</from-outcome>
		<to-view-id>pages/graficoPaciente.xhtml</to-view-id>
	</navigation-case>
</navigation-rule>
</faces-config>
